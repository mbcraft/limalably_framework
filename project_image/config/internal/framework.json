{
    "defaults" : {
        "execution_mode" : {
            "maintenance" : {
                "logging" : {
                    "type" : "together-file",
                    "min_level" : "debug",
                    "log_mode" : "normal"
                },
                "route_manager" : "LMaintenanceRouteManager"
            },
            "framework_development" : {
                "logging" : {
                    "type" : "output",
                    "min_level" : "debug"
                },
                "route_manager" : "LDefaultRouteManager"
            },
            "development" : {
                "logging" : {
                    "type" : "distinct-file",
                    "min_level" : "debug"
                },
                "route_manager" : "LDefaultRouteManager"
            },
            "testing" : {
                "logging" : {
                    "type" : "distinct-file",
                    "min_level" : "error"
                },
                "route_manager" : "LDefaultRouteManager"
            },
            "production" : {
                "logging" : {
                    "type" : "db",
                    "min_level" : "error"
                },
                "route_manager" : "LDefaultRouteManager"
            }
        },
        "routemap" : {
            "hash_db" : {
                "levels" : 5
            }
        },
        "logging" : {
            "distinct-file" : {
                "date_format" : "d/m/Y H:i:s",
                "log_format" : "[ %date% - %user% ] : %message%\n",
                "log_mode" : "normal",
                "max_mb" : 10,
                "log_folder" : "logs/",
                "min_level" : "debug"
            },
            "together-file" : {
                "date_format" : "d/m/Y H:i:s",
                "log_format" : "[ %date% - %user% - %level_string% ] : %message%\n",
                "log_mode" : "rolling",
                "max_mb" : 10,
                "log_folder" : "logs/",
                "min_level" : "debug"
            },
            "db" : {
                "log_mode" : "normal",
                "connection_name" : "default",
                "max_records" : 1000000,
                "table_name" : "logs",
                "min_level" : "debug"
            },
            "output" : {
                "min_level" : "debug"
            }
        },
        "hashmaps" : {
            "false_strings" : ["false","0","no",false]
        },
        "classloader" : {
            "code_file_ends_with" : [".class.php",".interface.php",".trait.php"]
        }
    }
}