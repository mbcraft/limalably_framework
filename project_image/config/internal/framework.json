{
    "defaults" : {
        "execution_mode" : {
            "maintenance" : {
                "logging" : {
                    "type" : "together-file",
                    "level" : "debug"
                },
                "route_manager" : "LMaintenanceRouteManager"
            },
            "framework_debug" : {
                "logging" : {
                    "type" : "together-file",
                    "level" : "debug"
                },
                "route_manager" : "LDefaultRouteManager"
            },
            "debug" : {
                "logging" : {
                    "type" : "distinct-file",
                    "level" : "debug"
                },
                "route_manager" : "LDefaultRouteManager"
            },
            "testing" : {
                "logging" : {
                    "type" : "distinct-file",
                    "level" : "error"
                },
                "route_manager" : "LDefaultRouteManager"
            },
            "production" : {
                "logging" : {
                    "type" : "db",
                    "database" : "default"
                },
                "route_manager" : "LDefaultRouteManager"
            }
        },
        "routemap" : {
            "hash_db" : {
                "levels" : 5
            }
        },
        "logging" : {
            "distict-file" : {
                "date_format" : "d/m/Y H:i:s",
                "log_format" : "[ %date - %user ] : %message",
                "log_mode" : "normal",
                "max_mb" : 10
            },
            "together-file" : {
                "date_format" : "d/m/Y H:i:s",
                "log_format" : "[ %date - %user - %level ] : %message",
                "log_mode" : "rolling",
                "max_mb" : 10
            },
            "mysql-db" : {
                "log_mode" : "normal",
                "COMMENT_max_records" : 100000
            }
        },
        "hashmaps" : {
            "false_strings" : ["false","0","no",false]
        }
    }
}